.ms_title_cls {
    width: 90%;
    height: 60rpx;
    border: 1px solid #999999;
    margin: 0 auto;
    margin-top: 20rpx;
    text-align: center;
    padding: 16rpx 0 0 0;
    font-weight: bold;
    font-size: 36rpx;
    border-radius: 20rpx;
}
.list_cls {
    width: 100%;
    height: 900rpx;
    margin-top: 10rpx;
    position: relative;
    white-space: nowrap;
}
.ms_tip_cls {
    text-align: right;
    display: block;
    height: 50rpx;
    border-top: 1px solid #999;
    padding: 25rpx 20rpx 0 0;
}
.boardItem_cls {
    display: block;
    position: relative;
    margin: 20rpx 0 0 0;
}