.ms_title_cls {
    width: 90%;
    height: 5vh;
    min-height: 60rpx;
    display: inline-block;
    line-height: 5vh;
    border: 1px solid #999999;
    margin-left: 5%;
    margin-top: 20rpx;
    text-align: center;
    font-weight: bold;
    font-size: 36rpx;
    border-radius: 20rpx;
}
.list_cls {
    width: 100%;
    height: 70vh;
    position: relative;
    margin: 1vh 0;
    white-space: nowrap;
}
.loading_cls {
    width: 100%;
    display: inline-block;
    text-align: center;
    color: #aaa;
    font-size: 25rpx;
}
.blank_cls {
    position: absolute;
    width: 260rpx;
    left: 245rpx;
    top: 48vh;
    font-size: 64rpx;
    color: #999;
    text-align: center;
}
.ms_tip_cls {
    width: 100%;
    height: 6vh;
    position: absolute;
    text-align: right;
    display: block;
    right: 0;
    bottom: 0;
    border-top: 1px solid #999;
    padding: 1vh 2vh;
    box-sizing: border-box;
}
.boardItem_cls {
    display: block;
    position: relative;
    margin: 20rpx 0 0 0;
}